{
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[php]": {
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "'Noto Mono', Menlo, Consolas, 'DejaVu Sans Mono', monospace",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.minimap.enabled": true,
  "editor.mouseWheelZoom": false,
  "editor.multiCursorModifier": "alt",
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.validate": ["javascript", "javascriptreact", "typescript"],
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/*.pyc": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.mvn": true,
    "**/.svn": true,
    "**/bower_components": false,
    "**/target": true
  },
  "files.insertFinalNewline": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.promptToSaveFilesBeforeCommit": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": false
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.historyExplorer.enabled": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "alternate",
  "javascript.showUnused": false,
  "multiclip.bufferSize": 3,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "'Noto Mono', 'Droid Sans Mono'",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.showUnused": false,
  "vsicons.dontShowNewVersionMessage": true,
  "vsicons.presets.jsOfficial": false,
  "vsicons.projectDetection.disableDetect": true,
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "statusBar.background": "#2f343f",
    "statusBar.debuggingBackground": "#511f1f",
    "statusBar.noFolderBackground": "#0A0A0D"
  },
  "workbench.colorTheme": "Atom One Dark",
  "workbench.iconTheme": "vscode-icons",
  "workbench.panel.location": "bottom",
  "workbench.statusBar.feedback.visible": false,
  "workbench.statusBar.visible": true,
  "workbench.welcome.enabled": false,
  "terminal.integrated.shell.linux": "/usr/bin/zsh"
}
